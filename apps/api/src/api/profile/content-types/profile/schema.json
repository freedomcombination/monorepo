{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "bio": {
      "type": "text"
    },
    "phone": {
      "type": "string"
    },
    "availableHours": {
      "type": "integer",
      "required": true,
      "default": 1,
      "min": 1
    },
    "heardFrom": {
      "type": "string"
    },
    "comment": {
      "type": "text"
    },
    "linkedin": {
      "type": "string"
    },
    "twitter": {
      "type": "string"
    },
    "instagram": {
      "type": "string"
    },
    "facebook": {
      "type": "string"
    },
    "inMailingList": {
      "type": "boolean",
      "default": false
    },
    "approved": {
      "type": "boolean",
      "default": false
    },
    "isPublic": {
      "type": "boolean",
      "default": false
    },
    "birthDate": {
      "type": "date"
    },
    "city": {
      "type": "string"
    },
    "platforms": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::platform.platform",
      "inversedBy": "volunteers"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "isVolunteer": {
      "type": "boolean"
    },
    "likedArts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::art.art",
      "inversedBy": "likers"
    },
    "ownedArts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::art.art",
      "mappedBy": "artist"
    },
    "likedBlogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "inversedBy": "likers"
    },
    "ownedBlogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog.blog",
      "mappedBy": "author"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "profile"
    },
    "feedbacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::feedback.feedback",
      "mappedBy": "editor"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "jobs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::job.job"
    },
    "profileStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "accepted",
        "rejected",
        "in-progress",
        "left",
        "awaiting",
        "approved"
      ],
      "default": "pending"
    },
    "observations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::observation.observation",
      "mappedBy": "profile"
    },
    "volunteerForm": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["files"]
    },
    "subscriber": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::subscriber.subscriber",
      "mappedBy": "profile"
    },
    "cv": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images", "files"]
    },
    "address": {
      "displayName": "Address",
      "type": "component",
      "repeatable": false,
      "component": "flow.address"
    },
    "locale": {
      "type": "enumeration",
      "enum": ["en", "tr", "nl"]
    },
    "team": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team.team",
      "inversedBy": "members"
    },
    "teams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::team.team",
      "mappedBy": "lead"
    }
  }
}
