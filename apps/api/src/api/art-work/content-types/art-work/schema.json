{
  "kind": "collectionType",
  "collectionName": "art_works",
  "info": {
    "singularName": "art-work",
    "pluralName": "art-works",
    "displayName": "Art Work"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "title_tr": {
      "type": "string"
    },
    "title_nl": {
      "type": "string"
    },
    "title_en": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "description_nl": {
      "type": "text"
    },
    "description_tr": {
      "type": "text"
    },
    "description_en": {
      "type": "text"
    },
    "content": {
      "type": "richtext"
    },
    "content_nl": {
      "type": "richtext"
    },
    "content_tr": {
      "type": "richtext"
    },
    "content_en": {
      "type": "richtext"
    },
    "views": {
      "type": "integer"
    },
    "likes": {
      "type": "integer"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "artist": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "approvalStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "approved",
        "rejected"
      ],
      "required": false,
      "default": "pending"
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "feedbacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::feedback.feedback",
      "mappedBy": "artWork"
    },
    "collection": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::collection.collection",
      "inversedBy": "artWorks"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "artWork"
    },
    "likers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "votes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vote.vote",
      "mappedBy": "artWork"
    }
  }
}
