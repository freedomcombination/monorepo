  "scripts": {
    "start": "npx nx serve:development",
    "build:admin": "npx nx build admin --prod --skip-nx-cache",
    "build:foundation": "npx nx build foundation --prod --skip-nx-cache && cp -R dist/apps/foundation/public dist/apps/foundation/.next",
    "build:kunsthalte": "npx nx build kunsthalte --prod --skip-nx-cache",
    "build:samenvvv": "npx nx build samenvvv --prod --skip-nx-cache",
    "admin": "npx nx run admin:serve:development",
    "foundation": "npx nx run foundation:serve:development",
    "kunsthalte": "npx nx run kunsthalte:serve:development",
    "samenvvv": "npx nx run samenvvv:serve:development",
    "tsc": "nx affected --target=tsc",
    "lint": "nx affected --target=lint --fix=true",
    "build": "npx nx affected --target=build --exclude=@strapi/admin,api",
    "build:api": "yarn --cwd apps/api build",
    "install:api": "yarn --cwd apps/api install",
    "test:admin": "yarn nx e2e admin-e2e --browser=chromium",
    "test:foundation": "yarn nx e2e foundation-e2e --browser=chromium",
    "test:kunsthalte": "yarn nx e2e kunsthalte-e2e --browser=chromium",
    "test:samenvvv": "yarn nx e2e samenvvv-e2e --browser=chromium",
    "api": "yarn --cwd apps/api develop",
    "db": "yarn --cwd apps/api prepare-db",
    "storybook": "npx nx run ui:storybook",
    "docker-up": "yarn --cwd apps/api docker-up",
    "docker-db": "yarn --cwd apps/api docker-db",
    "prepare": "husky install"
  },
