{
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=!api",
    "dev": "turbo run dev --concurrency=20",
    "dashboard": "turbo run dev --concurrency=20 --filter=dashboard --filter=api",
    "foundation": "turbo run dev --concurrency=20 --filter=foundation --filter=api",
    "kunsthalte": "turbo run dev --concurrency=20 --filter=kunsthalte --filter=api",
    "samenvvv": "turbo run dev --concurrency=20 --filter=samenvvv --filter=api",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "storybook": "turbo run storybook",
    "storybook-build": "turbo run storybook-build",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.9",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.41.0",
    "eslint-config-custom": "*",
    "prettier": "^2.8.8",
    "turbo": "^1.9.9"
  },
  "name": "turborepo",
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
